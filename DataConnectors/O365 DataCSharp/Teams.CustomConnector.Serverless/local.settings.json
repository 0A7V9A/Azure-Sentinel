{
  "IsEncrypted": false,
  "Values": {
    "AzureWebJobsStorage": "UseDevelopmentStorage=true", //not needed
    "FUNCTIONS_WORKER_RUNTIME": "dotnet", //not needed

    //Enable MSI for the Function app if you plan to use Key vault to store your keys else set to false and provide key values
    "KeyVaultEnabled": "false",
    "KeyVaultBaseUrl": "https://<Add Your KeyVault Base Url here>.vault.azure.net",

    //Key Vault enabled.
    //Client ID
    "ClientId": "<Add Your Client Id here>",

    //Key vault enabled.
    //Client Secret
    "ClientSecret": "<Add Your Client Secret here>",

    //Tennt Id
    "TenantId": "<Add Your Tenant Id here>",

    //AAD Instance
    "AADInstance": "https://login.microsoftonline.com/{0}",

    //Resource ID
    "ResourceId": "https://manage.office.com",

    //This is the subscription ID, read more on https://docs.microsoft.com/en-us/office/office-365-management-api/office-365-management-activity-api-reference
    "PublisherGUID": "<Add Your Publisher Guid here>",

    // "Start time and end time must be specified (or both omitted) and must be less than or equal to 24 hours apart, with the start time prior to  end time and start time no more than 7 days in the past.",
    "AuditLogExtractionStartDate": "4/15/2020 12:00:01 AM", //If not provided then Current UTC Time is taken as default.

    //Indicating the time range of content to return , read more on https://docs.microsoft.com/en-us/office/office-365-management-api/office-365-management-activity-api-reference
    "ConnectionIntervalinMinutes": "10",

    //Keyvault enabled.
    "StorageContainerConnectionString": "<Add Your Storage connection string here>",
    //Container name where log file will be written to store the last execution time
    "LogContainerName": "logs",
    //log file storing last execution time
    "LogFileName": "log.txt",
    // container to store the extracted logs per execution
    "DataContainerName": "data",
    //Enable archiving of the blobs
    "EnableArchiving": "true",

    //Enable direct injestion to the storage workspace
    "EnableDirectInjestionToWorkSpace": "true",
    //Customer ID
    "SentinelCustomerId": "<Add Your Sentinel workspace id here>",
    //Shared Key
    "SentinelSharedkey": "Add your shared key here"
  }
}