  {
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",

  "parameters": {
    "linkedTemplate_PAN-OS_Customconnector_Uri": {
      "defaultValue": "https://raw.githubusercontent.com/Azure/Azure-Sentinel/SOAR-connectors-Private-Preview/Playbooks/PaloAlto-PAN-OS/PaloAltoCustomConnector/azuredeploy.json",
      "type": "string",
      "metadata": {
        "description": "The Uri of the linked template for PAN-OS custom connector"
      }
    },
    "linkedTemplate_Playbook_PaloAlto-PAN-OS-BlockIP_Uri": {
      "defaultValue": "https://raw.githubusercontent.com/Azure/Azure-Sentinel/SOAR-connectors-Private-Preview/Playbooks/PaloAlto-PAN-OS/Playbooks/PaloAlto-PAN-OS-BlockIP/azuredeploy.json",
      "type": "string",
      "metadata": {
        "description": "The Uri of the linked template for playbook PaloAlto-PAN-OS-BlockIP"
      }
    },
    "linkedTemplate_Playbook_PaloAlto-PAN-OS-BlockURL_Uri": {
      "defaultValue": "https://raw.githubusercontent.com/Azure/Azure-Sentinel/SOAR-connectors-Private-Preview/Playbooks/PaloAlto-PAN-OS/Playbooks/PaloAlto-PAN-OS-BlockURL/azuredeploy.json",
      "type": "string",
      "metadata": {
        "description": "The Uri of the linked template for Playbook PaloAlto-PAN-OS-BlockURL"
      }
    },
    "linkedTemplate_Playbook_PaloAlto-PAN-OS-GetURLCategoryInfo_Uri": {
      "defaultValue": "https://raw.githubusercontent.com/Azure/Azure-Sentinel/SOAR-connectors-Private-Preview/Playbooks/PaloAlto-PAN-OS/Playbooks/PaloAlto-PAN-OS-GetURLCategoryInfo/azuredeploy.json",      
      "type": "string",
      "metadata": {
        "description": "The Uri of the linked template for Playbook PaloAlto-PAN-OS-GetURLCategoryInfo"
      }
    },
    "PaloAlto_PAN-OS_CustomConnector_Name": {
      "defaultValue": "PaloAlto-PAN-OS-Customconnector",
      "type": "String",
      "metadata": {
        "description": "Name of the Customconnector"
      }
    },
    "Service_EndPoint": {
      "defaultValue": "https://{yourPaloAltoDomain}",
      "type": "String",
      "metadata": {
        "description": "enter the okta endpoint (ex: https://{yourPaloAltoDomain})"
      }
    },
    "PaloAlto_PAN-OS_BlockIP_Playbook_Name": {
      "defaultValue": "PaloAlto-PAN-OS-BlockIP",
      "type": "String",
      "metadata": {
        "description": "Name of the Logic App/Playbook"
      }

    },
    "PaloAlto_PAN-OS_BlockURL_Playbook_Name": {
      "defaultValue": "PaloAlto-PAN-OS-BlockURL",
      "type": "String",
      "metadata": {
        "description": "Name of the Logic App/Playbook"
      }
    },
    "PaloAlto_PAN-OS_GetURLCategoryInfo_Playbook_Name": {
      "defaultValue": "PaloAlto-PAN-OS-GetURLCategoryInfo",
      "type": "String",
      "metadata": {
        "description": "Name of the Logic App/Playbook"
      }
    },
    "Teams_GroupId": {
      "defaultValue": "Teams_GroupId",
      "type": "string",
      "metadata": {
        "description": "GroupId of the Team channel"
      }
    },
    "Teams_ChannelId": {
      "defaultValue": "Teams_ChannelId",
      "type": "string",
      "metadata": {
        "description": "Team ChannelId"
      }
    },
    "Address_Group": {
      "defaultValue": "palaltogroup",
      "type": "String",
      "metadata": {
        "description": "Address Group"
      }
    }
  },

  "variables": {},
  "resources": [


    {
      "name": "linkedTemplate_PAN-OS_Customconnector_Uri",
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2018-05-01",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[parameters('linkedTemplate_PAN-OS_Customconnector_Uri')]"

        },
        "parameters": {
          "PaloAlto_PAN-OS_CustomConnector_Name": {
            "value": "[parameters('PaloAlto_PAN-OS_CustomConnector_Name')]"
          },
          "Service_EndPoint": {
            "value": "[parameters('Service_EndPoint')]"
          }

        }
        }
      },
    {
      "name": "linkedTemplate_Playbook_PaloAlto-PAN-OS-BlockIP_Uri",
      "dependsOn": [
        "[resourceId('Microsoft.Resources/deployments', 'linkedTemplate_PAN-OS_Customconnector_Uri')]"
      ],
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2018-05-01",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[parameters('linkedTemplate_Playbook_PaloAlto-PAN-OS-BlockIP_Uri')]"
        },
        "parameters": {
          "Playbook Name": {
            "Value": "[parameters('PaloAlto_PAN-OS_BlockIP_Playbook_Name')]"
          },
          "Teams GroupId": {
            "Value": "[parameters('Teams_GroupId')]"
          },
          "Teams ChannelId": {
            "Value": "[parameters('Teams_ChannelId')]"
          },
          "Address Group": {
            "Value": "[parameters('Address_Group')]"
          }
        }
      }
    },
    {
      "name": "linkedTemplate_Playbook_PaloAlto-PAN-OS-BlockURL_Uri",
      "dependsOn": [
        "[resourceId('Microsoft.Resources/deployments', 'linkedTemplate_PAN-OS_Customconnector_Uri')]"
      ],
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2018-05-01",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[parameters('linkedTemplate_Playbook_PaloAlto-PAN-OS-BlockURL_Uri')]"
        },
        "parameters": {
          "Playbook Name": {
            "Value": "[parameters('PaloAlto_PAN-OS_BlockURL_Playbook_Name')]"
          },
          "Teams GroupId": {
            "Value": "[parameters('Teams_GroupId')]"
          },
          "Teams ChannelId": {
            "Value": "[parameters('Teams_ChannelId')]"
          },
          "Address Group": {
            "Value": "[parameters('Address_Group')]"
          }
        }
      }
    },
    {
      "name": "linkedTemplate_Playbook_PaloAlto-PAN-OS-GetURLCategoryInfo_Uri",
      "dependsOn": [
        "[resourceId('Microsoft.Resources/deployments', 'linkedTemplate_PAN-OS_Customconnector_Uri')]"
      ],
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2018-05-01",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[parameters('linkedTemplate_Playbook_PaloAlto-PAN-OS-GetURLCategoryInfo_Uri')]"
        },
        "parameters": {
          "Playbook Name": {
            "Value": "[parameters('PaloAlto_PAN-OS_GetURLCategoryInfo_Playbook_Name')]"
          }
        }
      }
    }
  ]


}