                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "workspaceName": {
                        "value": "[parameters('workspaceName')]"
                    },
                    "location": {
                        "value": "[parameters('location')]"
                    },
                    "infobloxComputer": {
                        "value": "[parameters('infobloxComputer')]"
                    }
                }
            }
        },
         {
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2020-10-01",
