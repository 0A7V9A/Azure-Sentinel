{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "workspaceName": {
            "type": "string",
            "metadata": {
                "description": "Workspace name for Log Analytics where Sentinel is setup"
            }
        }
    },
      "resources": [
          {
          "name": "[concat(parameters('workspaceName'), '/Microsoft.SecurityInsights/ASimGenericCEFexclusion')]",
          "type": "Microsoft.OperationalInsights/workspaces/providers/Watchlists",
          "kind": "",
          "properties": {
              "displayName": "temp_ASimGenericCEFexclusion",
              "source": "ASimGenericCEFexclusion.csv",
              "description": "Testing Testing",
              "provider": "Custom",
              "isDeleted": false,
              "labels": [
              ],
              "defaultDuration": "P1000Y",
              "contentType": "Text/Csv",
              "numberOfLinesToSkip": 0,
              "itemsSearchKey": "ASimSchema",
              "rawContent": "ASimSchema,  DeviceVendor, DeviceProduct\r\n"
          },
          "apiVersion": "2021-03-01-preview"
          }       
      ]
  }  